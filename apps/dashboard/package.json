{
  "name": "@devflow/dashboard",
  "version": "1.0.0",
  "description": "Dashboard frontend for DevFlow Intelligence",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build && npm run optimize",
    "dev": "next dev",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:visual": "chromatic --exit-zero-on-changes",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:performance": "jest --testPathPattern=performance",
    "test:performance:benchmarks": "jest --testPathPattern=performance-benchmarks.test.ts",
    "audit:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "audit:bundle": "npm run build:analyze",
    "performance:ci": "node scripts/performance-ci.js",
    "performance:budget": "node -e \"const { performanceBudgetManager } = require('./src/services/performance/PerformanceBudgetManager'); console.log(JSON.stringify(performanceBudgetManager.exportBudgetsForCI(), null, 2));\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "optimize": "node scripts/optimize-build.js",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production",
    "clean": "rm -rf .next dist coverage storybook-static lighthouse-report.json"
  },
